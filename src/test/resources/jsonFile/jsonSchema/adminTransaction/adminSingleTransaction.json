{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome5",
  "definitions": {
    "Welcome5": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "data": {
          "$ref": "#/definitions/Data"
        },
        "message": {
          "type": "string"
        }
      },
      "required": [
        "data",
        "message"
      ],
      "title": "Welcome5"
    },
    "Data": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "location": {
          "type": "integer"
        },
        "fullname": {
          "type": "string"
        },
        "date": {
          "type": "string",
          "format": "date-time"
        },
        "phone": {
          "type": "string"
        },
        "address": {
          "type": "string"
        },
        "invoice": {
          "type": "integer"
        },
        "total": {
          "type": "integer"
        },
        "payment_token": {
          "type": "string",
          "format": "uuid"
        },
        "payment_link": {
          "type": "string",
          "format": "uri",
          "qt-uri-protocols": [
            "https"
          ]
        },
        "other": {
          "type": "string"
        },
        "additional": {
          "type": "integer"
        },
        "status": {
          "type": "integer"
        },
        "comment": {
          "type": "string"
        },
        "detail": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Detail"
          }
        }
      },
      "required": [
        "additional",
        "address",
        "comment",
        "date",
        "detail",
        "fullname",
        "id",
        "invoice",
        "location",
        "other",
        "payment_link",
        "payment_token",
        "phone",
        "status",
        "total"
      ],
      "title": "Data"
    },
    "Detail": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "vehicle_name": {
          "type": "string"
        },
        "service_name": {
          "type": "string"
        },
        "sub_total": {
          "type": "integer"
        }
      },
      "required": [
        "id",
        "service_name",
        "sub_total",
        "vehicle_name"
      ],
      "title": "Detail"
    }
  }
}
